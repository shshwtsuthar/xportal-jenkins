{
  "name": "xportal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:ci": "playwright test --reporter=junit --reporter=html",
    "test:api": "newman run newman/*.json --reporters cli,htmlextra",
    "prepare": "husky",
    "docker:build": "docker build -t xportal:latest .",
    "docker:run": "docker run -p 3000:3000 xportal:latest",
    "docker:compose": "docker-compose up -d",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx snyk test",
    "sonar:scan": "sonar-scanner",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
